clear all;
close all;
fid = fopen('/Users/suyaoji/Dropbox/research/board_design/JTAG_JAVA/Imager_SW/outputs/CalibrateADC/ADC_ext_sine_slow_p14d043_b1s3_1016_1734.csv','r');
c = fgetl(fid); 
f = fscanf(fid, '%f,%d,%d,%d,%d,%d,%d,%d,%d,%d,%d,%d, %d', [13 inf] );
t = f(1,:)';
dout= [ f(2,:);f(3,:);f(4,:);f(5,:);f(6,:);f(7,:);f(8,:);f(9,:);f(10,:);f(11,:);f(12,:)]';
clk_smp = [f(13,:)]';
[r,nbits] = size(dout);
fclose(fid);

weights = adc_calibration();
%%
close all;
N = 28087;
idx = 1;
data=zeros(1,N);
for i = 2:r
    if (clk_smp(i-1) == 1 && clk_smp(i)==0)
        data(idx)=dout(i,:)*weights'/2^(nbits-1);  
        idx= idx +1;
    end
    if idx > N
        break;
    end
end

fs = 71.2077e3;
figure;
plot(data);
snr=SNR(data,fs)
enob = (snr-1.76)/6.02
%%
clear all;
close all;
f=1e6;
fs= 1/96e-9;
t=(1:1024)/fs;
S = 0.7*sin(2*pi*f*t);
snr = SNR(S,fs);
